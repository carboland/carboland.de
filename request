<section id="certificate" class="pt_100">
<div class="container text-center">
<div class="heading-part">
<h2 class="main_title mb_20 wow fadeInUp" data-wow-delay="0.2s" style="visibility: hidden; animation-delay: 0.2s; animation-name: none;">ZERTIFIKAT</h2>
</div>
<div class="grid">
